<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="Assign-Access-Token-to-Response">
    <DisplayName>Assign Access Token to Response</DisplayName>
    <Set>
        <Payload contentType="application/json">{accesstoken}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>